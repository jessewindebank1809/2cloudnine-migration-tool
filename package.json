{
    "name": "2cloudnine-migration-tool",
    "version": "1.0.0",
    "description": "2cloudnine Migration Tool - Standalone Salesforce data migration platform",
    "main": "next.config.js",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev:env": "source ./scripts/load-env.sh && next dev",
        "dev:fresh": "./scripts/dev-server.sh",
        "dev:fresh:env": "./scripts/dev-server-env.sh",
        "dev:fresh:env:log": "./scripts/dev-server-env.sh > migration-output.log 2>&1",
        "dev:bun": "bun --bun run dev",
        "dev:bun:fresh": "bun --bun ./scripts/dev-server.sh",
        "build": "prisma generate && next build",
        "build:env": "next build",
        "build:bun": "bun --bun run build",
        "start": "next start",
        "start:env": "next start",
        "lint": "next lint",
        "type-check": "tsc --noEmit",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:migrate:env": "source ./scripts/load-env.sh && prisma migrate dev",
        "db:studio": "prisma studio",
        "db:studio:env": "source ./scripts/load-env.sh && prisma studio",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:e2e": "node tests/e2e/migration-results-validation.test.js",
        "test:quick": "node tests/e2e/quick-results-check.js",
        "test:e2e:puppeteer": "jest --config=tests/e2e/puppeteer/jest.config.js",
        "test:e2e:puppeteer:watch": "jest --config=tests/e2e/puppeteer/jest.config.js --watch",
        "test:e2e:puppeteer:debug": "DEBUG_TESTS=true jest --config=tests/e2e/puppeteer/jest.config.js",
        "test:e2e:puppeteer:headless": "HEADLESS=true jest --config=tests/e2e/puppeteer/jest.config.js",
        "test:e2e:puppeteer:screenshots": "CAPTURE_SCREENSHOTS=true jest --config=tests/e2e/puppeteer/jest.config.js",
        "test:e2e:auth": "jest --config=tests/e2e/puppeteer/jest.config.js tests/e2e/puppeteer/tests/auth.test.js",
        "test:e2e:orgs": "jest --config=tests/e2e/puppeteer/jest.config.js tests/e2e/puppeteer/tests/organizations.test.js",
        "test:e2e:migrations": "jest --config=tests/e2e/puppeteer/jest.config.js tests/e2e/puppeteer/tests/migrations.test.js",
        "test:e2e:templates": "jest --config=tests/e2e/puppeteer/jest.config.js tests/e2e/puppeteer/tests/templates.test.js",
        "test:e2e:analytics": "jest --config=tests/e2e/puppeteer/jest.config.js tests/e2e/puppeteer/tests/analytics.test.js",
        "test:e2e:errors": "jest --config=tests/e2e/puppeteer/jest.config.js tests/e2e/puppeteer/tests/error-handling.test.js",
        "test:e2e:performance": "jest --config=tests/e2e/puppeteer/jest.config.js tests/e2e/puppeteer/tests/performance.test.js",
        "test:e2e:browser": "jest --config=tests/e2e/puppeteer/jest.config.js tests/e2e/puppeteer/tests/cross-browser.test.js",
        "test:e2e:cleanup": "rm -rf tests/e2e/puppeteer/screenshots/* tests/e2e/puppeteer/downloads/*",
        "test:e2e:server": "node tests/e2e/puppeteer/start-test-server.js",
        "logs": "./scripts/logs-monitor.sh",
        "logs:live": "./scripts/logs-monitor.sh live",
        "logs:errors": "./scripts/logs-monitor.sh errors",
        "logs:migration": "./scripts/logs-monitor.sh migration",
        "logs:api": "./scripts/logs-monitor.sh api",
        "logs:recent": "./scripts/logs-monitor.sh recent",
        "logs:proxy": "node scripts/log-proxy-server.js",
        "logs:web": "node scripts/log-proxy-server.js 8080",
        "deploy": "./scripts/deploy.sh",
        "deploy:staging": "SKIP_TESTS=true ./scripts/deploy.sh staging",
        "deploy:production": "SKIP_TESTS=true ./scripts/deploy.sh production",
        "deploy:quick": "./scripts/quick-deploy.sh",
        "deploy:quick:staging": "./scripts/quick-deploy.sh staging",
        "deploy:quick:production": "./scripts/quick-deploy.sh production",
        "postinstall": "prisma generate",
        "bench:runtime": "node scripts/runtime-benchmark.js",
        "test:routes": "node scripts/route-speed-test.js"
    },
    "dependencies": {
        "@prisma/client": "^6.8.2",
        "@radix-ui/react-avatar": "^1.1.1",
        "@radix-ui/react-checkbox": "^1.3.2",
        "@radix-ui/react-collapsible": "^1.1.11",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-icons": "^1.3.2",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-popover": "^1.1.14",
        "@radix-ui/react-progress": "^1.1.0",
        "@radix-ui/react-radio-group": "^1.3.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.1.1",
        "@radix-ui/react-tabs": "^1.1.1",
        "@radix-ui/react-toast": "^1.2.2",
        "@sentry/nextjs": "^9.23.0",
        "@tanstack/react-query": "^5.59.14",
        "bcryptjs": "^2.4.3",
        "better-auth": "^1.2.8",
        "bullmq": "^5.54.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "date-fns": "^4.1.0",
        "fast-xml-parser": "^5.2.5",
        "framer-motion": "^12.12.2",
        "ioredis": "^5.4.1",
        "jose": "^5.9.3",
        "jsforce": "^2.0.0-beta.29",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.515.0",
        "next": "15.3.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.23.8",
        "zustand": "^4.5.5"
    },
    "devDependencies": {
        "@flydotio/dockerfile": "^0.7.10",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^22.7.5",
        "@types/node-fetch": "^2.6.12",
        "@types/pg": "^8.11.10",
        "@types/puppeteer": "^5.4.7",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "autoprefixer": "^10.4.20",
        "dotenv": "^16.5.0",
        "eslint": "^9.29.0",
        "eslint-config-next": "15.3.4",
        "jest": "^30.0.0",
        "jest-environment-jsdom": "^30.0.0",
        "jest-environment-puppeteer": "^11.0.0",
        "jest-image-snapshot": "^6.5.1",
        "jest-puppeteer": "^11.0.0",
        "node-fetch": "^2.7.0",
        "postcss": "^8.4.47",
        "prisma": "^6.8.2",
        "puppeteer": "^24.10.1",
        "tailwindcss": "^4.1.10",
        "typescript": "^5.6.3",
        "wait-port": "^1.1.0"
    },
    "overrides": {
        "brace-expansion": "^2.0.2",
        "jest-image-snapshot": {
            "jest": "$jest"
        }
    },
    "engines": {
        "node": ">=18.17.0"
    }
}
